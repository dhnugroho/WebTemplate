@{
    ViewBag.Title = "PARTICIPANT";
    Layout = "~/Views/Shared/_LayoutLg.cshtml";
}

<div style="height:100px; padding:70px 0 0 0;" class="well navbar-fixed-top">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/Menu/">Home</a></li>
        <li class="breadcrumb-item active">Generate File</li>
    </ol>
</div>

<div class="container">
    <div class="row">
        <div style="margin:100px 100px 0 100px;" class="col-sm-12 col-md-10">
            <form method="post" action="#" enctype="multipart/form-data">
                <div class="clearfix">
                    <div class="col-lg-12 cont-bg">
                        <h4 style="text-align:left; padding:2px;">GENERATE FILE</h4>
                    </div>
                    <div class="col-lg-12">
                        <div style="position:relative;">
                            <span class='label label-info' id="upload-file-info"></span>
                            &nbsp;
                            <span><button type="submit" style="border-radius:100px; width:150px;" class="btn btn-lg btn-info ">Download</button></span>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
